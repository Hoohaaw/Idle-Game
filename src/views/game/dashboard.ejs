<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <script src="/js/mission-script.js"></script>
  <title>Dashboard</title>

</head>
<body>
  <!-- Reward Modal -->
<div id="reward-modal" class="hidden">
  <div class="modal-content">
    <h2 id="reward-title">Mission Complete!</h2>
    <p id="reward-message">You earned XYZ!</p>
    <button id="close-reward">Claim Reward</button>
  </div>
</div>

<!-- Mission Modal -->
<div id="mission-modal" class="hidden">
  <div class="modal-content">
    <h2 id="mission-title">Mission Title</h2>
    <p id="mission-description">Mission Description</p>
    <p id="mission-reward">Reward: XYZ</p>
    <button id="start-mission">Start Mission</button>
    <button id="close-mission">Close</button>
  </div>
</div>

  <div id="dashboard-container">
    <div id="missions"></div>
    <div id="dashboard">
      <div id="world-pagination">
        <button id="pagination-1" class="selected">1</button>
        <button id="pagination-2">2</button>
        <button id="pagination-3">3</button>
        <button id="pagination-4">4</button>
      </div>
     
<!-- Map One -->
 <div id="map-one">
<button class="mission-button" id="mission-one">Mission One</button>
<button class="mission-button" id="mission-two">Mission Two</button>
<button class="mission-button" id="mission-three">Mission Three</button>
<button class="mission-button" id="mission-four">Mission Four</button>
<button class="mission-button" id="mission-five">Mission Five</button>
<button class="mission-button" id="mission-six">Mission Six</button>
<button class="mission-button" id="Map-one-boss">Final Boss</button>
</div>

<!-- Map Two -->
 <div id="map-two">
<button class="mission-button" id="mission-seven">Mission Seven</button>
<button class="mission-button" id="mission-eight">Mission Eight</button>
<button class="mission-button" id="mission-nine">Mission Nine</button>
<button class="mission-button" id="mission-ten">Mission Ten</button>
<button class="mission-button" id="mission-eleven">Mission Eleven</button>
<button class="mission-button" id="mission-twelve">Mission Twelve</button>
<button class="mission-button" id="Map-two-boss">Map Two Boss</button>
</div>

<!-- Map Three -->
 <div id="map-three">
<button class="mission-button" id="mission-thirteen">Mission Thirteen</button>
<button class="mission-button" id="mission-fourteen">Mission Fourteen</button>
<button class="mission-button" id="mission-fifteen">Mission Fifteen</button>
<button class="mission-button" id="mission-sixteen">Mission Sixteen</button>
<button class="mission-button" id="mission-seventeen">Mission Seventeen</button>
<button class="mission-button" id="mission-eighteen">Mission Eighteen</button>
<button class="mission-button" id="Map-three-boss">Map Three Boss</button>
</div>

<!-- Map Four -->
 <div id="map-four">
<button class="mission-button"id="mission-nineteen">Mission Nineteen</button>
<button class="mission-button"id="mission-twenty">Mission Twenty</button>
<button class="mission-button"id="mission-twenty-one">Mission Twenty-One</button>
